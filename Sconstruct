                    #CC = mingw_path + 'x86_64-w64-mingw32-gcc.exe',
                    #CXX = mingw_path + 'x86_64-w64-mingw32-g++.exe')
                    CC = 'gcc.exe',
                    CXX = 'g++.exe')

